    fi
else
    export WINEDIR="${START_WINE_PATH}/data/wine/"${DEFAULT_WINE}""
    clear
fi

var_ld_library_path_update "${START_WINE_PATH}/data/tools/utils/libs/i386"
var_ld_library_path_update "${START_WINE_PATH}/data/tools/utils/libs/x86-64"
