    def on_btn_resize1(self):

        cmd_cut = f"cut -d ' ' -f3-100 {sw_scripts}run.sh"
        proc = subprocess.run(cmd_cut, shell=True, stdout=subprocess.PIPE, encoding='UTF-8')
